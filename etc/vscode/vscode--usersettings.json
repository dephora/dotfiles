{
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.fontFamily": "'Hasklig', 'Source Code Pro', Menlo,monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": "500",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.lineHeight": 28,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 80,
  "editor.renderIndentGuides": false,
  "editor.tabSize": 2,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "Hasklig",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "500",
  "window.restoreFullscreen": true,
  "window.title": "${activeEditorLong}${separator}${rootName}",
  "window.zoomLevel": 0,
  "zenMode.hideTabs": false,
  "zenMode.hideStatusBar": false,
  "zenMode.hideActivityBar": false,
  "files.associations": {
    "*.tmpl": "html"
  },
  "files.trimFinalNewlines": true,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "html.format.wrapLineLength": 80,
  "scss.validate": true,
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": false,
    "editor.minimap.enabled": true
  },
  "cSpell.ignoreWords": ["Vuex", "vuetify", "styl", "nuxt"],
  "emmet.includeLanguages": {
    "vue": "html",
    "javascript": "javascriptreact"
  },
  "emmet.showAbbreviationSuggestions": true,
  "emmet.showExpandedAbbreviation": "always",
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "html",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    }
  ],
  "prettier.arrowParens": "always",
  "prettier.eslintIntegration": true,
  "prettier.singleQuote": true,
  "workbench.colorCustomizations": {
    "[One Dark Rain Coat Bright]": {
      // "activityBar.background": "#14191f",
      // "sideBar.background": "#161821",
      "editor.background": "#101119",
      "editor.lineHighlightBackground": "#161821",
      "sideBar.background": "#101119",      
      "tab.inactiveBackground": "#101119",
      "tab.activeBackground": "#101119",
      "editorGroupHeader.tabsBackground": "#101119"
      
    },
    "[TronLight]": {
      "activityBar.background": "#14191f",
      "activityBar.foreground": "#516a88",
      // "activityBar.border": "#516a88",
      "activityBarBadge.background": "#D2FF28",
      "activityBarBadge.foreground": "#14191f",
      // "activityBar.border": "#6ee2ff",
      "editor.background": "#14191f",
      "editorBracketMatch.border": "#ef626c",
      "editorCursor.foreground": "#D2FF28",
      "editorGroupHeader.tabsBackground": "#14191f",
      "editorLineNumber.foreground": "#516a88",
      "panel.background": "#14191f",
      "statusBar.background": "#14191f",
      "statusBar.foreground": "#516a88",
      "sideBar.background": "#14191f",
      "sideBar.foreground": "#ffffff",
      // "sideBar.foreground": "#ffffff",
      "tab.inactiveBackground": "#14191f",
      "tab.inactiveForeground": "#516a88",
      "tab.activeBackground": "#242d38",
      // "tab.activeBorder": "#aec2e0",
      "titleBar.activeBackground": "#14191f"
    }
  },
  // "editor.tokenColorCustomizations": {
  //     "[TronLight]": {
  //         "textMateRules": [
  //         ]
  //     }
  // },
  "editor.tokenColorCustomizations": {
    "[TronLight]": {
      "functions": "#ff6978",
      "numbers": "#12EAEA",
      "strings": "#e6ffff",
      "types": "#e6ffff",
      "variables": "#b6ffff"
    }
  },
  "code-runner.executorMap": {
    "javascript": "node",
    "elixir": "elixir",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "python": "python",
    "ruby": "ruby",
    "groovy": "groovy",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "bat": "cmd /c",
    "shellscript": "bash",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "racket": "racket"
  },
  "vsicons.dontShowNewVersionMessage": true,
  "explorer.confirmDragAndDrop": false,
  "breadcrumbs.enabled": false,
  "editor.renderWhitespace": "none",
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "grunt.autoDetect": "off",
"gulp.autoDetect": "off",
  "jake.autoDetect": "off",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "eq-material-theme-icons",
  "keyboard.touchbar.enabled": false,
  "rust-client.updateOnStartup": true,
  "rust-client.channel": "beta",
  "prettier.requireConfig": true,
  "prettier.trailingComma": "all",
  "cSpell.language": "en",
  "emmet.triggerExpansionOnTab": true,
  "workbench.colorTheme": "Dobri Next -C05- Electron",
  "reason.codelens.enabled": true,
  "typescript.implementationsCodeLens.enabled": true,
  "javascript.referencesCodeLens.enabled": true,
  "reason_language_server.per_value_codelens": true,
  "workbench.statusBar.feedback.visible": false,
  "eslint.run": "onSave",
  "prettier.tslintIntegration": true
}

